    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
    <script>
        var startDateString = document.getElementById("start-date").textContent;
        var endDateString = document.getElementById("end-date").textContent;

        var startTime = moment(startDateString, "DD/MM/YYYY HH:mm:ss");
        var endTime = moment(endDateString, "DD/MM/YYYY HH:mm:ss");

        var duration = moment.duration(endTime.diff(startTime));
        var elapsedHours = duration.hours();
        var elapsedMinutes = duration.minutes();
        var elapsedSeconds = duration.seconds();

        var elapsedTime = "";

        if (elapsedHours > 0) {
            elapsedTime += elapsedHours + "h ";
        }
        if (elapsedMinutes > 0) {
            elapsedTime += elapsedMinutes + "m ";
        }
        if (elapsedSeconds > 0 || elapsedTime === "") {
            elapsedTime += elapsedSeconds + "s";
        }

        document.getElementById("elapsed-time").textContent = elapsedTime.trim();
    </script>

